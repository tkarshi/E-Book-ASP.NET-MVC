<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>

    <!--Panel Setting-->
    <div id="slideout">
        <button type="button" class="btn setting-bt btn-default">
            <i class="fa fa-wrench" aria-hidden="true"></i>
        </button>
        <div class="box-colors">
            <h3 style="margin-top: 0">Colors</h3>
            <ul>
                <li class="theme_option cl-default active"
                    data-theme="@principalcolor"
                    data-theme2="@secondcolor">
                    <i class="fa fa-check" aria-hidden="true"></i>
                </li>
                <li class="theme_option cl-red"
                    data-theme="@principalcolor_red"
                    data-theme2="@secondcolor_red">
                    <i class="fa fa-check" aria-hidden="true"></i>
                </li>
                <li class="theme_option cl-orange"
                    data-theme="@principalcolor_orange"
                    data-theme2="@secondcolor_orange">
                    <i class="fa fa-check" aria-hidden="true"></i>
                </li>
                <li class="theme_option cl-violet"
                    data-theme="@principalcolor_violet"
                    data-theme2="@secondcolor_violet">
                    <i class="fa fa-check" aria-hidden="true"></i>
                </li>
                <li class="theme_option cl-blue"
                    data-theme="@principalcolor_blue"
                    data-theme2="@secondcolor_blue">
                    <i class="fa fa-check" aria-hidden="true"></i>
                </li>
                <li class="theme_option cl-brown"
                    data-theme="@principalcolor_brown"
                    data-theme2="@secondcolor_brown">
                    <i class="fa fa-check" aria-hidden="true"></i>
                </li>
            </ul>
        </div>
    </div>

    <!--Top Header-->
    <section class="top-header">
        <div class="container">
            <div class="col-xs-6 col-md-6 top-user">
                <div class="user-name">
                    <p>Welcome to BookStore</p>
                </div>
            </div>
            <div class="col-xs-6 col-md-6 mainmenu">
                <nav>
                    <ul>
                        <li>
                            <span class="plain-select">
                                <select class="language-select">
                                    <option label="Dollar(US)" selected="selected">
                                        Dollar(US)
                                    </option>
                                    <option label="Sterling(GBP)">Sterling(GBP)</option>
                                    <option label="Euro(EU)">Euro(EU)</option>
                                </select>
                            </span>
                        </li>
                        <li>
                            <a href="register.html">Register</a>
                        </li>
                        <li>
                            <a href="login.html">Login</a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
    </section>

    <div>
        @RenderBody()
    </div>

    <!-- NEWS LETTERS -->
    <section class="footer2 footer-newsletters">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="col-md-6 box-newsletters">
                        <h2 class="newsletters-title">
                            Subscribe now and get special offers
                        </h2>
                        <form name="newsForm" class="form-horizontal" method="post">
                            <div class="input-group">
                                <input type="email"
                                       class="form-control"
                                       name="email"
                                       placeholder="Email"
                                       required="" />
                            </div>
                            <div style="margin-top: 20px" class="form-group">
                                <!-- Button -->
                                <div class="col-sm-12 controls">
                                    <input type="submit"
                                           class="btn btn-success news-btn"
                                           value="Send" />
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-md-6 newsletters-img">
                        <img src="images/discount_bags_opt.jpg" alt="" />
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- FOOTER -->
    <section id="footer" class="ftr-2">
        <div class="container footer2">
            <div class="row">
                <div class="col-md-12 box-info">
                    <div class="col-md-4 box-info-footer">
                        <div class="col-md-4">
                            <i class="fa fa-bell-o" aria-hidden="true"></i>
                        </div>
                        <div class="col-md-8">
                            <p class="top-txt">Member Safe Shopping</p>
                            <p class="bottom-txt">Safe Shopping Guarantee</p>
                        </div>
                    </div>
                    <div class="col-md-4 box-info-footer">
                        <div class="col-md-4">
                            <i class="fa fa-clock-o" aria-hidden="true"></i>
                        </div>
                        <div class="col-md-8">
                            <p class="top-txt">30-day Return Products</p>
                            <p class="bottom-txt">Moneyback guarantee</p>
                        </div>
                    </div>
                    <div class="col-md-4 box-info-footer">
                        <div class="col-md-4">
                            <i class="fa fa-plane" aria-hidden="true"></i>
                        </div>
                        <div class="col-md-8">
                            <p class="top-txt">Free Shipping Worldwide</p>
                            <p class="bottom-txt">On oder over $70</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 footer-title">
                    <h2><span>Evo</span>Book</h2>
                </div>
                <div class="col-md-12">
                    <div class="col-md-3 footer-zone footer-about">
                        <h3>About Us</h3>
                        <p>
                            Lorem ipsum dolor sit amet, consectetur adipiscing elit. Quisque
                            arcu lorem, mattis quis convallis ac
                        </p>
                        <p>
                            Lorem ipsum dolor sit amet, consectetur adipiscing elit. Quisque
                            arcu lorem, mattis quis convallis ac
                        </p>

                        <div class="socialfooter">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                            <a href="#"><i class="fa fa-dribbble"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                            <a href="#"><i class="fa fa-instagram"></i></a>
                            <a href="#"><i class="fa fa-google"></i></a>
                        </div>
                    </div>
                    <div class="col-md-3 footer-about">
                        <h3>EvoBook NEWS</h3>
                        <div class="col-md-12 footer-box-news-book">
                            <div class="col-md-4 footer-small-img">
                                <img alt="" src="images/footer_news3.jpg" />
                            </div>
                            <div class="col-md-8 footer-small-desc">
                                <a href="#">Amazing Places</a>
                                <p>Mar 22, 2015</p>
                                <a class="r-more" href="#">read more</a>
                            </div>
                        </div>
                        <div class="col-md-12 footer-box-news-book">
                            <div class="col-md-4 footer-small-img">
                                <img alt="" src="images/footer_news2.jpg" />
                            </div>
                            <div class="col-md-8 footer-small-desc">
                                <a href="#">Amazing Places</a>
                                <p>Mar 26, 2015</p>
                                <a class="r-more" href="#">read more</a>
                            </div>
                        </div>
                        <div class="col-md-12 footer-box-news-book">
                            <div class="col-md-4 footer-small-img">
                                <img alt="" src="images/footer_news1.jpg" />
                            </div>
                            <div class="col-md-8 footer-small-desc">
                                <a href="#">Amazing Places</a>
                                <p>Mar 26, 2015</p>
                                <a class="r-more" href="#">read more</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 footer-zone footer-about">
                        <h3>CONTACTS</h3>
                        <p>
                            <i class="fa fa-map-marker"></i>Address: Broadway Road 35-127
                        </p>
                        <p><i class="fa fa-phone"></i>Phone: +001 3072341187</p>
                        <p><i class="fa fa-envelope-o"></i>Mail: info@evobook.com</p>
                        <p><i class="fa fa-globe"></i>evobook@world.com</p>
                    </div>
                    <div class="col-md-3 box-tags footer-about">
                        <p><i class="fa fa-headphones" aria-hidden="true"></i></p>
                        <h3>+44 345 01 989</h3>
                    </div>
                    <div class="footer-map"><img src="images/map.png" alt="map" /></div>
                </div>
            </div>
        </div>
        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="col-xs-6 copyright">
                            ThemeOxygen © 2017 Name All Rights Reserved
                        </div>
                        <div class="col-xs-6 payment-card">
                            <i class="fa fa-2x fa-cc-visa"></i>
                            <i class="fa fa-2x fa-cc-mastercard"></i>
                            <i class="fa fa-2x fa-cc-amex"></i>
                            <i class="fa fa-2x fa-cc-paypal"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="../../../../../ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script type="text/javascript"
            src="Scripts/modernizr.2.5.3.min.js"></script>
    <script src="../../../../../maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>

    <!--Mini Flexslide-->
    <script src="Scripts/minislide/jquery.flexslider.js"
            type="text/javascript"></script>

    <script src="Scripts/minislide/shCore.js" type="text/javascript"></script>
    <script src="Scripts/minislide/shBrushXml.js"
            type="text/javascript"></script>
    <script src="Scripts/minislide/shBrushJScript.js"
            type="text/javascript"></script>

    <!-- OwnCarousel -->
    <script src="Scripts/owncarousel/owl.carousel.js"></script>

    <script src="../../../../../cdn.jsdelivr.net/jquery.mixitup/latest/jquery.mixitup.min.js"></script>
    <script src="Scripts/turn.min.js"></script>

    <!-- Animation Text -->
    <script src="Scripts/textmove/assets/jquery.fittext.js"
            type="text/javascript"></script>
    <script src="Scripts/textmove/assets/jquery.lettering.js"
            type="text/javascript"></script>
    <script src="Scripts/textmove/jquery.textillate.js"
            type="text/javascript"></script>

    <!-- General Script -->
    <script src="Scripts/main.js" type="text/javascript"></script>
</body>

